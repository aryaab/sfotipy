@font-face {
	font-family: 'icomoon';
	src:url('../fonts/icomoon.eot?-ofh2ft');
	src:url('../fonts/icomoon.eot?#iefix-ofh2ft') format('embedded-opentype'),
		url('../fonts/icomoon.woff?-ofh2ft') format('woff'),
		url('../fonts/icomoon.ttf?-ofh2ft') format('truetype'),
		url('../fonts/icomoon.svg?-ofh2ft#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

